#: data/multiplayer/units.ruleset:438 data/multiplayer/units.ruleset:484
#: data/multiplayer/units.ruleset:545 data/multiplayer/units.ruleset:2042
#: data/multiplayer/units.ruleset:2384 data/multiplayer/units.ruleset:2422
msgid "green"
msgstr "kokematon"

#: data/civ1/units.ruleset:46 data/civ1/units.ruleset:1250
#: data/civ2/units.ruleset:52 data/civ2/units.ruleset:1955
#: data/civ2/units.ruleset:2050 data/classic/units.ruleset:58
#: data/experimental/units.ruleset:61 data/civ2civ3/units.ruleset:60
#: data/civ2civ3/units.ruleset:2070 data/multiplayer/units.ruleset:57
msgid "veteran"
msgstr "konkari"

#. TRANS: Unit class: used adjectivally
#: data/civ1/units.ruleset:131 data/civ2/units.ruleset:145
#: data/classic/units.ruleset:151 data/experimental/units.ruleset:154
#: data/civ2civ3/units.ruleset:153 data/multiplayer/units.ruleset:150
msgid "?unitclass:Land"
msgstr "maavoimien"

#. TRANS: Unit class: used adjectivally
#: data/civ1/units.ruleset:140 data/civ2/units.ruleset:154
#: data/classic/units.ruleset:160 data/experimental/units.ruleset:174
#: data/civ2civ3/units.ruleset:186 data/alien/units.ruleset:151
#: data/multiplayer/units.ruleset:159
msgid "?unitclass:Sea"
msgstr "merivoimien"

#. TRANS: Unit class: used adjectivally
#: data/civ1/units.ruleset:147 data/civ2/units.ruleset:168
#: data/classic/units.ruleset:181 data/experimental/units.ruleset:195
#: data/civ2civ3/units.ruleset:208 data/multiplayer/units.ruleset:180
msgid "?unitclass:Air"
msgstr "ilmavoimien"

#. TRANS: Unit class: used adjectivally
#: data/civ1/units.ruleset:155 data/civ2/units.ruleset:137
#: data/classic/units.ruleset:143 data/experimental/units.ruleset:146
