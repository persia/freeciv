#: data/civ2/nations.ruleset:387 data/nation/carthaginian.ruleset:5
msgid "Carthaginian"
msgstr "カルタゴ人"

#: data/civ2/nations.ruleset:388 data/nation/carthaginian.ruleset:6
msgid "?plural:Carthaginians"
msgstr "カルタゴ人"

# clefs
