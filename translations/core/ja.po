"delegate take <player-name>\n"
"delegate restore\n"
"delegate show <player-name>"
msgstr ""
"delegate to <接続名> [プレイヤー名]\n"
"delegate cancel [プレイヤー名]\n"
"delegate take <プレイヤー名>\n"
"delegate restore\n"
