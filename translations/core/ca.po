msgid "beginner"
msgstr ""

#: data/sandbox/units.ruleset:630 data/civ2civ3/units.ruleset:620
msgid "seasoned"
msgstr ""

#: data/sandbox/units.ruleset:630 data/civ2civ3/units.ruleset:620
msgid "senior"
msgstr ""

#: data/sandbox/units.ruleset:630 data/civ2civ3/units.ruleset:620
msgid "expert"
